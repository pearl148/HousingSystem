@model IEnumerable<Expense>
@{
    ViewData["Title"] = "Details";

}
<partial name="_Notification"></partial>
<br><br>

   
<table class="table table-border table-striped rounded-2" style="width:100%;text-align:center;background-color: rgba(100,100,100, 0.1) !important">
    <thead class="rounded-2">
        <tr>
            <th>
                ExpenseId
            </th>
            <th>
                Item
            </th>
            <th>
                Date
            </th>
            <th>
                Expense Month Year
            </th>
            <th>
                Expense Amount
            </th>
            <th>
                Expense Account Head
            </th>
            
            <th>Remark
            </th>
             <th>Expense Proof
            </th>
            </tr>
    </thead>
    <tbody class="rounded-2">
    @foreach(var obj in Model)
    {
        <tr >
            <td width="30%">
                @obj.ExpenseId
            </td>
             <td width="30%">
                @obj.ExpenseItem
            </td>
            <td width="30%">
                @obj.ExpenseDate
            </td>
            <td width="30%">
                @obj.ExpenseMonthYear
            </td>
            <td width="30%">
                @obj.ExpenseAmount
            </td>
            <td width="30%">
                @obj.ExpenseAccountHead
            </td>
            <td width="30%">
                @obj.ExpenseRemark
            </td>
            <td width="30%">
                
                <img src="data:image/png;base64,@Convert.ToBase64String(obj.ExpenseProofData)" alt="Expense Proof" height="50%" width="50%"/>

            </td>
           
        </tr>
    }
    </tbody>
</table>
